{
  "name": "bumblebee-discord",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch": "tsc --watch",
    "debug": "nodemon --watch ./dist --inspect=0.0.0.0:5858 ./dist/server.js",
    "compile": "tsc",
    "compile-and-run": "tsc && node ./dist/server.js",
    "start": "if test \"$NODE_ENV\" = \"production\" ; then npm run start:prod ; else npm run start:dev;  fi",
    "start:dev": "nodemon -e ts --exec \"npm run compile-and-run\"",
    "start:prod": "node ./dist/server.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@types/memorystream": "^0.3.0",
    "discord.js": "^12.5.1",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.3.0",
    "memorystream": "^0.3.1",
    "moment": "^2.29.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "tsyringe": "^4.5.0",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.35",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.28.0",
    "prettier": "^2.3.1",
    "typescript": "^4.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "tmp/"
    ],
    "delay": "500"
  }
}
